package src.shooter {	import flash.display.MovieClip;	import flash.events.Event;	import src.shooter.Game;	public class RadioSignal extends MovieClip{				var life:int = 1;		public function RadioSignal(){			addEventListener(Event.ENTER_FRAME, onFrame, false, 0, true);		}		private function onFrame(e:Event){											if ( life >= 40 ){				removeEventListener(Event.ENTER_FRAME, onFrame);				Game.remove(this);			}			else{				life++;			}		}			}}