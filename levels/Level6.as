package src.shooter.levels {	import flash.display.MovieClip;	import flash.events.Event;		import src.core.Game;		import src.shooter.Obstacle;	import src.shooter.Enemy;	public class Level6 extends Level{		//Level 1-6 has the player scrambling to free the exit as enemies keep coming		public function Level6(playerX, playerY, playerDirection){			super(playerX, playerY, playerDirection);		}				override function waitForAdded(e:Event){			super.waitForAdded(e);						//Next Level			nextLevel.x = 640;			nextLevel.y = 200;			//nextLevel.rotation = 0;			//Walls			var wall0:Obstacle = new Obstacle("junglewall",0,320,0,0);			wall0.rotation = -90;			bottomLayer.addChild(wall0);			var wall1:Obstacle = new Obstacle("junglewall",0,640,0,0);			wall1.rotation = -90;			bottomLayer.addChild(wall1);			var wall3:Obstacle = new Obstacle("junglewall",320,480,0,0);			wall3.rotation = 180;			bottomLayer.addChild(wall3);			var wall4:Obstacle = new Obstacle("junglewall",640,480,0,0);			wall4.rotation = 180;			bottomLayer.addChild(wall4);			var wall5:Obstacle = new Obstacle("junglewall",640,-160,0,0);			wall5.rotation = 90;			bottomLayer.addChild(wall5);			var iwall1:Obstacle = new Obstacle("iwall",0,0,640,40);			bottomLayer.addChild(iwall1);			var iwall2:Obstacle = new Obstacle("iwall",600,0,40,480);			bottomLayer.addChild(iwall2);						var hutWall:Obstacle = new Obstacle("iwall",107,95,63,110);			bottomLayer.addChild(hutWall);			var hutWall2:Obstacle = new Obstacle("iwall",176,318,110,63);			bottomLayer.addChild(hutWall2);									//Obstacles			var crate0:Obstacle = new Obstacle("crate", 444,209,0,0,true,false,true);			bottomLayer.addChild(crate0);			var crate1:Obstacle = new Obstacle("crate", 417,261,0,0,true,false,true);			bottomLayer.addChild(crate1);			var crate2:Obstacle = new Obstacle("crate", 395,340,0,0,true,false,true);			bottomLayer.addChild(crate2);			var crate3:Obstacle = new Obstacle("crate", 335,297,0,0,true,false,true);			bottomLayer.addChild(crate3);			var crate4:Obstacle = new Obstacle("crate", 348,223,0,0,true,false,true);			bottomLayer.addChild(crate4);			var crate5:Obstacle = new Obstacle("crate", 387,159,0,0,true,false,true);			bottomLayer.addChild(crate5);			var tree0:Obstacle = new Obstacle("treetrunk", 450,358,0,0);			tree0.rotation = 80;			bottomLayer.addChild(tree0);			var tree1:Obstacle = new Obstacle("treetrunk", 512,160,0,0);			tree1.rotation = -10;			bottomLayer.addChild(tree1);		}				override function exitTrigger(){			super.exitTrigger();			//var level7 = new Level7();			Game.gameRef.loadLevel("level7", false); //, 80, 240, 90		}				override function counterTrigger(){						if (counter == 36 ){				var enemy1:Enemy = new Enemy(spawnPoint1.x,spawnPoint1.y, 1, 1, null);				topLayer.addChild(enemy1);						var enemy2:Enemy = new Enemy(spawnPoint2.x,spawnPoint2.y, 1, 1, null);				topLayer.addChild(enemy2);					}						if (counter % 360 == 0 && counter != 0 ){				var enemy3:Enemy = new Enemy(spawnPoint1.x,spawnPoint1.y, 1, 1, null);				topLayer.addChild(enemy3);						var enemy4:Enemy = new Enemy(spawnPoint2.x,spawnPoint2.y, 1, 1, null);				topLayer.addChild(enemy4);					}								}	}}