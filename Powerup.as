package src.shooter {	import flash.display.MovieClip;	import flash.events.*	//import flash.utils.Timer;	//import flash.display.BlendMode;	import src.core.Game;	public class Powerup extends MovieClip{		var type:String;		public function Powerup(_type, xPos, yPos){								type = _type					if ( type == "random" ){				if ( Math.random() > 0.5)	{ type = "health" }				else						{ type = "xp" }			}							//Visuals			switch (type){				case "health":					var health:Health = new Health();					addChild(health);					break;				case "xp":					var xp:XP = new XP();					addChild(xp);					break;			}						ShooterGame.powerupList.push(this);			x = xPos;			y = yPos;									//addEventListener(Event.ENTER_FRAME, onFrame, false, 0, true);		}//		private function onFrame(e:Event){//			//		}		public function snatch(){			switch (type){				case "health":					Player.playerRef.hp += 25;					break;				case "xp":					Player.xp += 100;					break;			}			//Effect			//Play Sound			//s_pickup.play()			remove();					}		public function remove(){			//Remove Listeners			//removeEventListener(Event.ENTER_FRAME, onFrame);			//Search list and delete from array			for(var i=0; i < ShooterGame.powerupList.length; i++)			{				if ( ShooterGame.powerupList[i]==this) { ShooterGame.powerupList.splice(i,1) };			}									Game.remove(this);								}	}}