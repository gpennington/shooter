package src.shooter {	import flash.display.MovieClip;	//import flash.events.Event;	//import flash.geom.Point;	//import src.shooter.Projectile;	public class Rocket extends Projectile{		public function Rocket(shooter, a, s, p){			super(shooter, a, s, p);			life = 96		}		override public function move(){						super.move();						//speed x radians			y += speed * Math.sin((angle - 90) * (Math.PI / 180));			x += speed * Math.cos((angle - 90) * (Math.PI / 180));								}		override public function kill(){						//Add Explode			var explosion:Explosion = new Explosion(5, shooter.friendly);			explosion.x = x;			explosion.y = y;			Game.currentLevel.topLayer.addChild(explosion);			super.kill();					}		override public function remove(){						//Remove from Array			for (var i=0; i < Game.bulletList.length; i++) {				if(Game.bulletList[i]==this) { Game.bulletList.splice(i,1) }			}			super.remove();					}					}}